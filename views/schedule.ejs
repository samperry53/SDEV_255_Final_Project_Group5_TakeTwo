<!-- schedule.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Schedule</title>
</head>
<body>
    <h1>Student Schedule</h1>
    <ul>
        <% courses.forEach(course => { %>
            <li>
                <h2><%= course.name %></h2>
            </li>
        <% }); %>
    </ul>
</body>
</html>